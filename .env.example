# Настройки базы данных
DB_HOST=postgres
DB_PORT=5432
DB_NAME=netflow_db
DB_USER=netflow_user
DB_PASSWORD=your_strong_password_here

# Схема базы данных
DB_SCHEMA=netflow

# Настройки NetFlow
NETFLOW_PORT=2055
BUFFER_SIZE=65535
BATCH_SIZE=1000
FLUSH_INTERVAL=5

# Логирование
LOG_LEVEL=WARNING
LOG_FILE=/app/logs/netflow.log

# Очистка данных
ENABLE_CLEANUP=false
RETENTION_DAYS=30
